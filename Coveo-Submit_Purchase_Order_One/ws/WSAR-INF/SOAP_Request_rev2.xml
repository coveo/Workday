<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="2.0"
	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <xsl:param name="item"/>
  <xsl:param name="snow_num"/>
  <xsl:param name="unit_cost"/>
  <xsl:param name="manufacturer"/>
  <xsl:param name="description"/>
  <xsl:param name="Supplier_ID"/>
  <xsl:param name="Supplier_Category"/>
  <xsl:param name="Payment_Terms"/>
  <xsl:param name="Currency"/>
  <xsl:param name="PO_Issue_Option"/>
  <xsl:param name="Company_Restrictions"/>
  <xsl:param name="Spend_Category"/>
  <xsl:param name="Employee_ID"/>
  <xsl:param name="bill_to_address"/>
  <xsl:param name="document_date"/>
<xsl:template match="/">
<env:Envelope xmlns:env="http://schemas.xmlsoap.org/soap/envelope/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
<env:Body>
<wd:Submit_Purchase_Order_Request xmlns:wd="urn:com.workday/bsvc" wd:Add_Only="true" wd:version="v37.2">
<wd:Business_Process_Parameters>
<wd:Auto_Complete>false</wd:Auto_Complete>
</wd:Business_Process_Parameters>
<wd:Purchase_Order_Data>
<wd:Submit>false</wd:Submit>
<wd:Locked_in_Workday>false</wd:Locked_in_Workday>
<wd:Item_Reference>
<wd:ID wd:type="Purchase_Item_ID"><xsl:value-of select="$item"/></wd:ID>
</wd:Item_Reference>
<wd:Internal_Memo>
<wd:ID wd:type="string"><xsl:value-of select="$snow_num"/></wd:ID>
</wd:Internal_Memo>
<wd:Unit_Cost>
<wd:ID wd:type="decimal"><xsl:value-of select="$unit_cost"/></wd:ID>
</wd:Unit_Cost>
<wd:Manufacturer_Reference_ID>
<wd:ID wd:type="Manufacturer_Reference_ID"><xsl:value-of select="$manufacturer"/></wd:ID>
</wd:Manufacturer_Reference_ID>
<wd:Item_Description>
<wd:ID wd:type="string"><xsl:value-of select="$description"/></wd:ID>
</wd:Item_Description>
<wd:Supplier_Reference>
<wd:ID wd:type="Supplier_ID"><xsl:value-of select="$Supplier_ID"/></wd:ID>
</wd:Supplier_Reference>
<wd:Supplier_Category_ID>
<wd:ID wd:type="Supplier_Category_ID"><xsl:value-of select="$Supplier_Category"/></wd:ID>
</wd:Supplier_Category_ID>
<wd:Payment_Terms_Reference>
<wd:ID wd:type="Payment_Terms_ID"><xsl:value-of select="$Payment_Terms"/></wd:ID>
</wd:Payment_Terms_Reference>
<wd:Currency_Reference>
<wd:ID wd:type="CurrencyObject"><xsl:value-of select="$Currency"/></wd:ID>
</wd:Currency_Reference>
<wd:Issue_Option_Reference>
<wd:ID wd:type="Purchase_Order_Issue_OptionObject"><xsl:value-of select="$PO_Issue_Option"/></wd:ID>
</wd:Issue_Option_Reference>
<wd:Company_Reference_ID>
<wd:ID wd:type="Company_Reference_ID"><xsl:value-of select="$Company_Restrictions"/></wd:ID>
</wd:Company_Reference_ID>
<wd:Resource_Category_Reference>
<wd:ID wd:type="Spend_CategoryObject"><xsl:value-of select="$Spend_Category"/></wd:ID>
</wd:Resource_Category_Reference>
<wd:Employee_ID>
<wd:ID wd:type="Employee_ID"><xsl:value-of select="$Employee_ID"/></wd:ID>
</wd:Employee_ID>
<wd:Bill_To_Address_Data>
<wd:ID wd:type="Bill_To_Address_Reference"><xsl:value-of select="$bill_to_address"/></wd:ID>
</wd:Bill_To_Address_Data>
<wd:Document_Date>
<wd:ID wd:type="date"><xsl:value-of select="$document_date"/></wd:ID>
</wd:Document_Date>
<wd:Service_Line_Replacement_Data>
<wd:Due_Date/>
<wd:Start_Date></wd:Start_Date>
<wd:End_Date></wd:End_Date>
<wd:Prepaid>false</wd:Prepaid>
<wd:Retention>false</wd:Retention>
</wd:Service_Line_Replacement_Data>
</wd:Purchase_Order_Data>
</wd:Submit_Purchase_Order_Request>
</env:Body>
</env:Envelope>
</xsl:template>
</xsl:stylesheet>